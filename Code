def printf(string):
    if not isinstance(string, str):
        raise TypeError("Ошибка: вывод ожидается строка (printf)")
    print(f"Вывод: {string}")

def declination_hours_safe(hours):
    if not isinstance(hours, int):
        raise TypeError("Входные данные для часов должны быть числами")
    if (hours >= 24 or hours < 0):
        raise ValueError("Неправильный формат ввода: часы должны быть числом от 0 до 23")
    hours %= 12
    if (hours == 0):
        return "0 часов"
    elif (hours == 1):
        return "1 час"
    elif (2 <= hours < 5):
        return f"{hours} часа"
    elif (5 <= hours < 12):
        return f"{hours} часов"

def declination_minutes_safe(minutes):
    if not isinstance(minutes, int):
        raise TypeError("Входные данные для минут должны быть числами")
    if (minutes >= 60 or minutes < 0):
        raise ValueError("Неправильный формат ввода: минуты должны быть числом от 0 до 59")
    if (minutes == 0):
        return "ровно"
    elif (minutes%10 == 1):
        return f"{minutes} минута"
    elif (2 <= minutes%10 < 5):
        return f"{minutes} минуты"
    return return f"{minutes} минут"

def daytime_safe(hours):
    if not isinstance(hours, int):
        raise TypeError("Входные данные для часов должны быть числами")
    if (0 <= hours < 6):
        return "ночи"
    elif (6 <= hours < 12):
        return "утра"
    elif (12 <= hours < 18):
        return "дня"
    elif (18 <= hours < 24):
        return "вечера"

def isnumber(char):
    if (ord('0') <= ord(char) <= ord('9')):
        return True
    return False

def pars(string):
    count_of_probel = 0
    count_number = 0
    buff = ""
    string += " "
    for i in range(len(string)):
        if ((not isnumber(string[i]) and string[i] != ' ') or (string[i] == ' ' and count_of_probel > 1)):
            raise ValueError("Неправильный формат ввода: введены недопустимые символы")
        if (string[i] == ' '):
            count_of_probel += 1
        if (isnumber(string[i])):
            count_number += 1
            buff += string[i]
        else:
            if (count_number > 2 and buff[0] == '0'):
                raise ValueError("Неправильный формат ввода: введено число начанающееся на ноль")
            count_number = 0
            buff = ""

def main():
    print("Программа преобразовывает два числа (часы и минуты) в текстовое описание времени суток.")
    print("Введите два целых числа через пробел (часы и минуты):")
    
    try:
        str_in = input()
        pars(str_in)
        hours_str, minutes_str = str_in.split()
        hours = int(hours_str)
        minutes = int(minutes_str)

        if not (0 <= hours <= 23):
            raise ValueError("Неправильный формат ввода: часы должны быть числом от 0 до 23")
        if not (0 <= minutes <= 59):
            raise ValueError("Неправильный формат ввода: минуты должны быть числом от 0 до 59")

        if hours == 0 and minutes == 0:
            printf("полночь")
            return 0
        elif hours == 12 and minutes == 0:
            printf("полдень")
            return 0
        hours_string = declination_hours_safe(hours)
        minutes_string = declination_minutes_safe(minutes)
        daytime_string = daytime_safe(hours)
        if (minutes_string == "ровно"):
            final_output = f"({minutes_string} {hours_string} {daytime_string})".strip()
        else:
            final_output = f"({hours_string} {minutes_string} {daytime_string})".strip()
        printf(final_output)

    except ValueError as e:
        print(f"Ошибка ввода данных: {e}")
        return -1
    except TypeError as e:
        print(f"Ошибка типа данных: {e}")
        return -1
    except IndexError as e:
        print(f"Внутренняя ошибка обработки часов: {e}")
        return -1
    except Exception as e:
        print(f"Произошла непредвиденная ошибка: {e}")
        return -1
        
    return 0
    
if (__name__ == "__main__"):
    status = main()
